  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-react": "^6.11.1",
    "install": "^0.8.1",
    "postcss-cli": "^2.5.2",
    "postcss-sorting": "^1.6.1",
    "resolve-url-loader": "^1.6.0",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-scss": "^1.3.4"
  }

{
  "name": "recallab-ui",
  "version": "0.1.0",
  "description": "Frontend for recallab.",
  "main": "src/js/index.js",
  "scripts": {
    "build": "rm -rf build/*.* && NODE_ENV=production webpack -p",
    "dev": "webpack-dev-server --progress --colors --hot --inline --module-bind",
    "regenerate": "rm -rf node_modules && npm install",
    "stylefmt": ". ./stylefmt.sh",
    "post-sort": "postcss --use postcss-sorting .postcss-sort.json -r",
    "style-watch": "nodemon ./node_modules/.bin/stylelint src/styles/**/*.*"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stefan-vatov/recallab-ui"
  },
  "author": "Stefan Vatov",
  "license": "MIT",
  "dependencies": {
    "12factor-config": "^1.2.1",
    "auth0-lock": "^10.2.1",
    "autoprefixer": "^6.3.6",
    "axios": "^0.15.1",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.6",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-tcomb": "^0.3.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "browser-sync": "^2.12.12",
    "browser-sync-webpack-plugin": "^1.0.3",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.33.0",
    "font-awesome": "^4.6.3",
    "font-awesome-sass-loader": "^1.0.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "megatype": "^1.0.10",
    "mocha": "^3.0.2",
    "node-bourbon": "^4.2.8",
    "node-sass": "^3.7.0",
    "nodemon": "^1.10.2",
    "normalize-scss": "^5.0.3",
    "normalizr": "^2.2.1",
    "postcss-loader": "^1.0.0",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",

{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "ignore": false,
  "passPerPreset": true,
  "plugins" : [
    "syntax-flow",
    "tcomb",
    "transform-flow-strip-types",
    "espower",
    "transform-runtime",
    "transform-regenerator",
    "syntax-jsx",
    "babel-plugin-transform-es2015-modules-commonjs",
    "transform-react-jsx",
    "transform-object-rest-spread"
  ]
}
